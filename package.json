{
  "name": "my-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "android:dev": "node ./cli.js dev android",
    "android:build": "react-router build && npx cap sync android && npx cap open android",
    "android:generate-icons": "npx @capacitor/assets generate --assetPath ./icons --android",
    "android:init": "npm i -D @capacitor/android && npx cap add android",
    "ios:dev": "node ./cli.js dev ios",
    "ios:build": "react-router build && npx cap sync ios && npx cap open ios",
    "ios:generate-icons": "npx @capacitor/assets generate --assetPath ./icons --ios",
    "ios:init": "npm i -D @capacitor/ios && npx cap add ios",
    "types:check": "react-router typegen && tsc --noEmit",
    "types:show-config": "tsc --showConfig",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:inspect-config": "npx @eslint/config-inspector",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "code:check": "npm run types:check && npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "@react-router/node": "^7.1.1",
    "isbot": "^5.1.19",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.1"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^6.2.0",
    "@capacitor/core": "^6.2.0",
    "@eslint/config-inspector": "^0.6.0",
    "@eslint/js": "^9.17.0",
    "@react-router/dev": "^7.1.1",
    "@react-router/fs-routes": "^7.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "commander": "^12.1.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.6",
    "vite-imagetools": "^7.0.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
